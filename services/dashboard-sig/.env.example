# Variables d'environnement pour le développement
# Copier ce fichier en .env.local et adapter les valeurs

# URL de base de l'API Gateway
VITE_API_BASE_URL=http://localhost:8080

# Mode de l'application (development, production)
VITE_APP_MODE=development

# Activer les données de démonstration (true/false)
VITE_USE_MOCK_DATA=true

# Configuration de la carte
VITE_MAP_DEFAULT_CENTER_LAT=31.7917
VITE_MAP_DEFAULT_CENTER_LNG=-7.0926
VITE_MAP_DEFAULT_ZOOM=6
